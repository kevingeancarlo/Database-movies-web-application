<!DOCTYPE html>
<html lang="en" >
<head>
  <meta charset="UTF-8">
  <title>Seccion CLIENTES</title>
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css'>
<link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,500,700,900&amp;subset=latin,latin-ext'><link rel="stylesheet" href="resources/css/style_login_register.css">

</head>
<body>
  <div class="login-form">
    <h2>Cliente Cinema UD</h2>
    <form action="/auth" method="POST">
      <input type="text" name="user" id="user" placeholder="Correo" required>
      <input type="password" name="pass" id="pass" placeholder="ContraseÃ±a" required>
      <input type="submit" class="btn-login" value="Login">
    </form>
    <a href="register" class="btn-register">Register</a>

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

   <% if (typeof alert != "undefined")  { %>
    <script>
    Swal.fire({
      title:'<%= alertTitle %>',
      text:'<%= alertMessage %>',
      icon:'<%= alertIcon %>',
      showConfirmButton: <%= showConfirmButton %>,
      timer: <%= timer %>
    }).then(()=>{
      window.location='/<%= ruta %>'
    })
    </script>
  <% } %>
  </div>
</body>
</html>
